// tests/test_interpolation.yen

let name = "World";
let age = 30;
let height = 1.75;
let isStudent = true;
let nothing = None;
let myList = [1, 2, 3];

print "Hello, ${name}!"; // Expected: Hello, World!
print "Age: ${age}"; // Expected: Age: 30
print "Height: ${height}"; // Expected: Height: 1.75
print "Student: ${isStudent}"; // Expected: Student: true
print "Nothing: ${nothing}"; // Expected: Nothing: null

let result = "${age} + ${height}";
print result; // Expected: 30 + 1.75

print "10 + 5.5 = ${10 + 5.5}"; // Expression interpolation
