func testar_lista() {
    let frutas = ["Maçã", "Banana"];
    print "Antes do push:";
    print frutas;

    push(frutas, "Uva");
    print "Depois do push:";
    print frutas;

    let removida = pop(frutas);
    print "Depois do pop:";
    print frutas;
    print "Removida: " + removida;
}

testar_lista();
